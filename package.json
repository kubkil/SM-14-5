{
  "name": "SM-14-5",
  "version": "1.0.0",
  "description": "https://kubkil.github.io/SM-14-5/",
  "main": "index.js",
  "scripts": {
    "init": "npm-run-all -s init:*",
    "init:folders": "mkdir sass css js images",
    "init:files": "touch index.html sass/style.scss css/style.css js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "watch": "npm-run-all -p watch:*",
    "watch:sass": "node-sass -w --source-map true --output-style compressed scss/ -o css/",
    "watch:bs": "browser-sync start --server --files css/*.css *.html --browser chrome",
    "build": "babel src -d lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kubkil/SM-14-5.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kubkil/SM-14-5/issues"
  },
  "homepage": "https://github.com/kubkil/SM-14-5#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.23.6",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2"
  }
}
